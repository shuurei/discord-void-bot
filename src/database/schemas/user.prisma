model User {
    id    String @id
    flags Int    @default(0)

    guilds Member[]

    blacklistsAsTarget  Blacklist[] @relation("Target")
    blacklistsAsAuthor  Blacklist[] @relation("Author")
    blacklistsAsCleaner Blacklist[] @relation("Cleaner")

    tagAssignedAt DateTime?
    createdAt     DateTime  @default(now())

    @@map("user")
}
